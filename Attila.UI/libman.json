{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "jquery@3.3.1",
      "destination": "wwwroot/lib/jquery/"
    },
    {
      "library": "jquery-validate@1.19.1",
      "destination": "wwwroot/lib/jquery-validate/",
      "files": [
        "jquery.validate.js",
        "jquery.validate.min.js",
        "additional-methods.js",
        "additional-methods.min.js"
      ]
    },
    {
      "provider": "unpkg",
      "library": "jquery-validation@1.19.1",
      "destination": "wwwroot/lib/jquery-validation/",
      "files": [
        "dist/jquery.validate.js",
        "dist/jquery.validate.min.js",
        "dist/additional-methods.js",
        "dist/additional-methods.min.js"
      ]
    },
    {
      "library": "jquery-ajax-unobtrusive@3.2.6",
      "destination": "wwwroot/lib/jquery-ajax-unobtrusive/"
    },
    {
      "library": "twitter-bootstrap@4.4.1",
      "destination": "wwwroot/lib/bootstrap/",
      "files": [
        "css/bootstrap.min.css",
        "css/bootstrap.min.css.map",
        "js/bootstrap.bundle.js",
        "js/bootstrap.bundle.min.js",
        "js/bootstrap.bundle.min.js.map"
      ]
    },
    {
      "provider": "jsdelivr",
      "library": "font-awesome@4.7.0",
      "destination": "wwwroot/lib/font-awesome/",
      "files": [
        "css/font-awesome.css",
        "css/font-awesome.css.map",
        "css/font-awesome.min.css",
        "fonts/fontawesome-webfont.eot",
        "fonts/fontawesome-webfont.svg",
        "fonts/fontawesome-webfont.ttf",
        "fonts/fontawesome-webfont.woff",
        "fonts/fontawesome-webfont.woff2",
        "fonts/FontAwesome.otf"
      ]
    },
    {
      "provider": "filesystem",
      "library": "Vendors/Telerik UI for ASP.NET Core/",
      "destination": "wwwroot/lib/kendo-ui/",
      "files": [
        "js/kendo.all.min.js",
        "js/kendo.all.min.js.map",
        "js/kendo.aspnetmvc.min.js",
        "js/kendo.aspnetmvc.min.js.map",
        "styles/kendo.default-v2.min.css",
        "styles/kendo.default-v2.min.css.map",
        "styles/fonts/glyphs/KendoUIGlyphs.eot",
        "styles/fonts/glyphs/KendoUIGlyphs.svg",
        "styles/fonts/glyphs/KendoUIGlyphs.ttf",
        "styles/fonts/glyphs/KendoUIGlyphs.woff",
        "styles/fonts/glyphs/WebComponentsIcons.eot",
        "styles/fonts/glyphs/WebComponentsIcons.svg",
        "styles/fonts/glyphs/WebComponentsIcons.ttf",
        "styles/fonts/glyphs/WebComponentsIcons.woff",
        "styles/Default/editor.png",
        "styles/Default/imagebrowser.png",
        "styles/Default/indeterminate.gif",
        "styles/Default/loading.gif",
        "styles/Default/loading_2x.gif",
        "styles/Default/loading-image.gif",
        "styles/Default/markers.png",
        "styles/Default/markers_2x.png",
        "styles/Default/slider-h.gif",
        "styles/Default/slider-v.gif",
        "styles/Default/sprite.png",
        "styles/Default/sprite_2x.png",
        "styles/Default/sprite_kpi.png",
        "styles/Default/sprite_kpi_2x.png"
      ]
    }
  ]
}